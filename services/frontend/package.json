{
    "name": "ufincs-frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "postinstall": "patch-package",
        "start": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "storybook": "storybook dev -p 9009 --no-open",
        "build-storybook": "storybook build",
        "test": "CI=true TZ=UTC vitest",
        "test:watch": "TZ=UTC vitest",
        "test:storybook": "test-storybook --url http://127.0.0.1:9009",
        "tsc:check": "tsc -b",
        "tsc:watch": "tsc -b --watch",
        "lint": "eslint 'src/**/*.[j|t]s*' 'cypress/**/*.[j|t]s'",
        "lint:watch": "nodemon -e js,ts,tsx --exec 'npm run lint || exit 0'",
        "lint:styles": "stylelint 'src/**/*.scss'",
        "lint:all": "npm run lint && npm run lint:styles && npm run tsc:check && npm run prettier:check",
        "lint:aider": "sh ./scripts/aider_lint.sh",
        "prettier": "prettier --write src cypress",
        "prettier:check": "prettier --check src cypress",
        "ci": "npm-run-all --parallel lint lint:styles tsc:check prettier:check test",
        "playwright:install-browsers": "playwright install",
        "cypress": "cypress run",
        "cypress:open": "cypress open",
        "cypress:parallel:split": "node ./scripts/split_spec_files.js",
        "cypress:parallel:run": "node ./scripts/cypress_run_parallel_single.mjs"
    },
